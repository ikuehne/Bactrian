#lang racket
(define do (lambda (a b) a b))

(define loop
    (lambda (n f)
        (if (= n 0) ()
            (do (f n)
                (loop (- n 1) f)))))

(define ~ (lambda (x) (- 0 x)))

(define forward-loop
  (lambda (n f)
          (loop n (lambda (x)
                    (f (+ 1 (- 10 x)))))))

(forward-loop 10 print)
